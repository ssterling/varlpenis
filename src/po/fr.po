#: src/main.c:198
#, c-format
msgid "    -c       output color\n"
msgstr "    -c       sortir en couleur\n"

#: src/main.c:200
#, c-format
msgid "    -e num   set length of semen string\n"
msgstr "    -e num   fixer la longueur du sperme\n"

#: src/main.c:202
#, c-format
msgid "    -f       output fullwidth characters\n"
msgstr "    -f       sortir des caractères pleine chasse\n"

#: src/main.c:204
#, c-format
msgid "    -h       show this message\n"
msgstr "    -h       afficher ce message\n"

#: src/main.c:205
#, c-format
msgid "    -l num   set length of shaft\n"
msgstr "    -l num   fixer la longueur du corps\n"

#: src/main.c:206
#, c-format
msgid "    -v       show copyright and version info\n"
msgstr "    -v       afficher informations de copyright et version\n"

#: src/main.c:164
#, c-format
msgid "Error: argument to option `-%c' is too large (above `UINT_MAX').\n"
msgstr "Erreur : le paramètre de l'option `-%c' est trop grand (plus de `UINT_MAX')\n"

#: src/main.c:154
#, c-format
msgid "Error: argument to option `-%c' must be a positive integer\n"
msgstr "Erreur : le paramètre de l'option `-%c' doit être un entier positif\n"

#: src/main.c:159
#, c-format
msgid "Error: argument to option `-%c' must be zero or a positive integer\n"
msgstr "Erreur : le paramètre de l'option `-%c' doit être zéro ou un entier positif\n"

#: src/main.c:178
#, c-format
msgid "Error: bogus error code received from options parser\n"
msgstr "Erreur : faux code d'erreur de l'analyseur a été reçu\n"

#: src/main.c:149
#, c-format
msgid "Error: option `-%c' requires a positive integer as an argument\n"
msgstr "Erreur : l'option `-%c' nécessite un paramètre qui est un entier positif\n"

#: src/main.c:170
#, c-format
msgid "Error: unknown option `-%c'\n"
msgstr "Erreur : option inconnue `-%c'\n"

#: src/main.c:173
#, c-format
msgid "Error: unknown option char `\\x%x'\n"
msgstr "Erreur : caractère d'option inconnu `\\x%x'\n"

#: src/strings.h:15
msgid "Generate an ASCII-art penis of arbitrary length"
msgstr "Générer de l'ASCII-art de n'importe quelle longeur"

