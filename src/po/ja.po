#: src/main.c:198
#, c-format
msgid "    -c       output color\n"
msgstr "    -c       カラーで出力する\n"

#: src/main.c:200
#, c-format
msgid "    -e num   set length of semen string\n"
msgstr "    -e num   精液の長さを設定する\n"

#: src/main.c:202
#, c-format
msgid "    -f       output fullwidth characters\n"
msgstr "    -f       全角で出力する\n"

#: src/main.c:204
#, c-format
msgid "    -h       show this message\n"
msgstr "    -h       このヘルプ情報を表示する\n"

#: src/main.c:205
#, c-format
msgid "    -l num   set length of shaft\n"
msgstr "    -l num   シャフトの長さを設定する\n"

#: src/main.c:206
#, c-format
msgid "    -v       show copyright and version info\n"
msgstr "    -v       著作権とバージョン情報を表示する\n"

#: src/main.c:164
#, c-format
msgid "Error: argument to option `-%c' is too large (above `UINT_MAX').\n"
msgstr "エラー：`-%c'の値は大きすぎます（>UINT_MAX）\n"

#: src/main.c:154
#, c-format
msgid "Error: argument to option `-%c' must be a positive integer\n"
msgstr "エラー：`-%c'の値は正整数でなればいけません\n"

#: src/main.c:159
#, c-format
msgid "Error: argument to option `-%c' must be zero or a positive integer\n"
msgstr "エラー：`-%c'の値は0・正整数でなればいけません\n"

#: src/main.c:178
#, c-format
msgid "Error: bogus error code received from options parser\n"
msgstr "エラー：引数構文解析器から変なエラーコードである\n"

#: src/main.c:149
#, c-format
msgid "Error: option `-%c' requires a positive integer as an argument\n"
msgstr "エラー：`-%c'は正整数が必要です\n"

#: src/main.c:170
#, c-format
msgid "Error: unknown option `-%c'\n"
msgstr "エラー：`-%c'は不明なオプションです\n"

#: src/main.c:173
#, c-format
msgid "Error: unknown option char `\\x%x'\n"
msgstr "エラー：`\\x%x'は不明なオプション文字です\n"

#: src/strings.h:15
msgid "Generate an ASCII-art penis of arbitrary length"
msgstr "ASCII-artの陰茎を生成します"

